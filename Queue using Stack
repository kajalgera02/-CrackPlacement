class StackQueue {
    stack<int>st1,st2;
public:
    StackQueue() {
    }
    
    void push(int x) {
     while(!st1.empty()){
        st2.push(st1.top());
        st1.pop();
     }
     st1.push(x);
     while(!st2.empty()){
        st1.push(st2.top());
        st2.pop();
     }
    }
    
    int pop() {
     if(st1.empty()){
        return -1;
     }
     int topElement=st1.top();
     st1.pop();
     return topElement;
    }
    
    int peek() {
     if(st1.empty()){
        return -1;
     }
     return st1.top();
    }
    
    bool isEmpty() {
      return st1.empty();
    }
};
